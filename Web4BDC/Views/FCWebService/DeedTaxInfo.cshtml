        function GetDataByTXM(txm,slbh,ry_id) {
            $.ajax({
                url: '@Url.Action("GetDeedTaskInfo")' + '/?txm=' + txm + '&ry_id=ry_id'+"&slbh="+slbh,
                type: 'POST',
                success: function (data) {
                    ShowTaxData(data);
                }
            });

        }

