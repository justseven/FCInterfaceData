        {

        }


        function GetDataBySLBH(slbh) {
            $.ajax({
                url: '@Url.Action("GetDeedTaskBySLBH")' + '/?SLBH=' + slbh,
                type: 'POST',
                async: false,
                success: function (data) {
                    if (data && data.TaxXML)
                        ShowTaxData(data.TaxXML);
                    if (data && data.TXM)
                        $("#txm").val(data.TXM);
                }
            });
        }

        function ShowBarCode() {
            var txm = $("#txm").val();
            if (!txm || txm.length == 0) {
                alert("请输入条形码");
                return;
            }
            else {
                $("#barcode").html("");

                var imgStr = "<div><img src='../FCWebService/GetBarCodeImg?txm=" + txm + "'/></div>";


                $("#barcode").append(imgStr);
            }
        }

